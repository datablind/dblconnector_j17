<!-- settings.dblind.xml -->

<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
   https://maven.apache.org/xsd/settings-1.0.0.xsd">
   <localRepository/>
   <interactiveMode/>
   <usePluginRegistry/>
   <offline/>
   <pluginGroups>
       <pluginGroup>org.mule.tools</pluginGroup>
   </pluginGroups>
   <servers>
       <server>
           <id>anypoint-exchange</id>
           <username>${env.ANYPOINT_USERNAME}</username>
           <password>${env.ANYPOINT_PASSWORD}</password>
       </server>
       <server>
           <id>anypoint-exchange-v3</id>
           <username>${env.ANYPOINT_USERNAME}</username>
           <password>${env.ANYPOINT_PASSWORD}</password>
       </server>
       <server>
           <id>exchange-server</id>
           <username>${env.ANYPOINT_USERNAME}</username>
           <password>${env.ANYPOINT_PASSWORD}</password>
       </server>
<!--        <server>
           <id>MuleRepository</id>
           <username>${env.MULEENTREPO_USERNAME}</username>
           <password>${env.MULEENTREPO_PASSWORD}</password>
       </server>
 -->       
        <server>
           <id>datablindrepo</id>
           <username>${env.DBLREPO_USERNAME}</username>
           <password>${env.DBLREPO_PASSWORD}</password>
       </server>
       
   </servers>
   <mirrors/>
   <profiles>
       <profile>
           <id>Mule</id>
           <activation>
               <activeByDefault>true</activeByDefault>
           </activation>
           <repositories>
<!--               <repository>
                   <id>MuleRepository</id>
                   <name>MuleRepository</name>
                   <url>https://repository.mulesoft.org/nexus-ee/content/repositories/releases-ee/</url>
                   <layout>default</layout>
                   <releases>
                       <enabled>true</enabled>
                   </releases>
                   <snapshots>
                       <enabled>true</enabled>
                   </snapshots>
               </repository> -->
               <repository>
                   <id>datablindrepo</id>
                   <name>GitHub DataBlind Repository</name>
                   <url>https://maven.pkg.github.com/datablind/datacrypt</url>
                   <layout>default</layout>
                   <releases>
                       <enabled>true</enabled>
                   </releases>
                   <snapshots>
                       <enabled>true</enabled>
                   </snapshots>
               </repository>
           </repositories>
       </profile> 
       <profile>
           <id>mule-extra-repos</id>
           <activation>
               <activeByDefault>true</activeByDefault>
           </activation>
           <repositories>
               <repository>
                   <id>mule-public</id>
                   <url> https://repository.mulesoft.org/nexus/content/repositories/public </url>
               </repository>
           </repositories>
           <pluginRepositories>
               <pluginRepository>
                   <id>mule-public</id>
                   <url> https://repository.mulesoft.org/nexus/content/repositories/public </url>
               </pluginRepository>
           </pluginRepositories>
       </profile>
   </profiles>
   <activeProfiles/>
</settings>
